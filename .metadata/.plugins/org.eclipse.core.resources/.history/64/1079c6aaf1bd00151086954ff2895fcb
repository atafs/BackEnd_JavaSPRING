package dao;

public class Triangle {
	
	//ATTRIBUTES
	private String type;
	
	//GETTERS AND SETTERS
	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	/** PROCEDURE: print a message */
	public void draw() {
		System.out.println("Triangle draw!!");
	}
}
